<%- include('../partials/header') %>

      <div class="starter-template">
        <h1>Tabuada do número <%= num %></h1>
        <p class="lead">Gere a tabuada de um número de 0 a 100.</p>
        <table class="table table-striped table-bordered table-hover">
          <% for(let i=1; i <= 100; i++) { %>
            <tr>
              <td><%= num %></td>
              <td>&nbsp; x &nbsp;</td>
              <td><%= i %></td>
              <td>&nbsp; = &nbsp;</td>
              <td><%= num * i%></td>
            </tr>
          <% } %>
        </table>
      </div>

<%- include('../partials/footer') %>
